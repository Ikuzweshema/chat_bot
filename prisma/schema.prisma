generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Chart {
  id        String   @id @map("_id")
  title     String
  path      String
  messages  Json[]
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
